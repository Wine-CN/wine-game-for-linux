        <div id="focus_close"><img src="http://bmp.ali213.net/Pics/x.jpg" /></div>
        <script>
			var focus_over = document.getElementById("focus_over");
			var focus_close = document.getElementById("focus_close"); 
			function splay(){
				if(focus_over.style.visibility = "hidden"){
					focus_over.style.visibility = "visible";
				}
				if(focus_close.style.visibility = "hidden"){
					focus_close.style.visibility = "visible";
				}
			}
			function shidden(){
				if(focus_over.style.visibility != "hidden"){
					focus_over.style.visibility = "hidden";
				}
				if(focus_close.style.visibility != "hidden"){
					focus_close.style.visibility = "hidden";
				}
			}
			var wait=60;
			for(i=1;i<=wait;i++) {
			    window.setTimeout("doUpdate(" + i + ")", i * 100);
			}
			function doUpdate(num) {
			    if(num == wait) {
			    	shidden();
			    }
			}
		</script>        
    
